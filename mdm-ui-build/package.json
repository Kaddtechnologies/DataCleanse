{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node src/scripts/dev.js",
    "dev:noopen": "next dev --turbo -p 9003",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build --turbo",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test:persistence": "npx ts-node tests/run-tests.ts",
    "test:session": "npx ts-node -e \"import('./tests/session-persistence.test').then(m => m.runSessionPersistenceTests())\"",
    "test:bulk": "npx ts-node -e \"import('./tests/bulk-actions.test').then(m => m.runBulkActionsTests())\"",
    "test:load": "npx ts-node -e \"import('./tests/session-load.test').then(m => m.runSessionLoadTests())\"",
    "db:setup": "bash setup-database.sh",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && ./setup-database.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@genkit-ai/googleai": "^1.8.0",
